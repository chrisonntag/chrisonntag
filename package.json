{
  "name": "repo",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf tmp/*",
    "build:parcel": "parcel build public/js/app.js --no-content-hash --no-minify --no-source-maps --out-dir public/js --out-file app.bundle.js --public-url ./",
    "build": "npm run clean && npm run build:parcel"
  },
  "dependencies": {
    "gsap": "^3.6.0",
    "imagesloaded": "^4.1.4",
    "parcel-bundler": "^1.12.4"
  }
}
